struct Header {
    name: string
    value: string
}

enum Method {
    GET
    HEAD
    POST
    PUT
    DELETE
    CONNECT
    OPTIONS
    TRACE
    PATCH
}

struct Request {
    url: string
    method: Method
    headers: Header[]
}

fn capitalize(input: string) : string {}

fn fetch(url) {
    let headers = [Header("Content-Type", "application/json")]
    let request = Request(
        url: url,
        method: Method.GET,
        headers: headers
    )
}
