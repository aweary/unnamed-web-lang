type Landmark {}

component View() {}
component HStack() {}
component VStack() {}
component Text() {}

component LandmarkRow(landmark: Landmark) {
    let body = View {
        match landmark.name {
            "foo" => HStack {
                Text("Hello there")
            }
            _ => VStack {
                Text("Hello there!")
            }
        }
    }
    return body
}