enum Either<T, U> {
    Left(T),
    Right(U),
}

fn with_either_number_or_string(
    either: Either<number, string>,
) {}


fn main() {
    with_either_number_or_string(Either.Left(1));
    with_either_number_or_string(Either.Right("right"));
    // Oops! Wrong type
    with_either_number_or_string(Either.Left("oops!"));
}