component Button(label: string, onPress: Function) : Node {
  return (
    <button onClick={onPress} class="btn">{label}</button>
  )
}


component Counter<T>(
  count: Number,
  increment: Function 
) : Node {
  return (
    <div>
      <title class="counter">
        This is a counter
      </title>
      <Button label={count} onPress={increment} />
    </div>
  )
}

